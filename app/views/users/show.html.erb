<section class='user-info-wrapper'>
  <ul id='user-info'>
    <p><%= user.name %></p>
    <p><%= user.email %></p>
    <p><%= image_tag user.thumbnail %></p>
  </ul>

  <p class='header-rest-visited'>5 LAST VISITED RESTAURANTS</p>
<section class='single-card'>
  <section id='Visits'>
    <% user.visited_restaurants.each do |restaurant| %>
    <p class='visit'><%= link_to restaurant.name, restaurant.url, target: :_blank %></p>
    <% end %>
  </section>
</section>


<%= button_to 'Log Out', logout_path, method: :get, class: 'button' %>

</section>
