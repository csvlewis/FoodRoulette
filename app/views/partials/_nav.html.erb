<header class="header">
  <section class="header-logo">
    <%= link_to root_path do %>
      <%= image_tag('logo.png', class: "home-header-logo-1", alt: "Roulette Logo") %>
      <%= image_tag('logo-3.png', class: "home-header-logo-2", alt: "Roulette Logo 2") %>
    <% end %>
  </section>
  <nav class="nav">
    <ul class="navlinks">
        <li class="navlink">
          <%= link_to "Home", root_path, method: :get %>
        </li>
        <li class="navlink">
          <%= link_to "Profile", profile_path, method: :get %>
        </li>
        <li class="navlink">
          <%= link_to 'About', about_path, method: :get%>
        </li>
    </ul>
    <select>
     <option value="" selected="selected">Select</option>

     <option value="/">Home</option>
     <option value="/profile">Profile</option>
     <option value="/about">About</option>
   </select>
  </nav>
</header>
